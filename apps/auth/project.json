{
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/auth/src",
  "projectType": "application",
  "targets": {
    "ts-build": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "rm -rf dist/auth",
          "tsc -p apps/auth/tsconfig.app.json",
          "tsc-alias -p apps/auth/tsconfig.app.json"
        ],
        "parallel": false
      }
    },
    "ts-build-2": {
      "executor": "@nrwl/js:tsc",
      "outputs": ["{options.outputPath}"],
      "dependsOn": [
        {
          "projects": "dependencies",
          "target": "build"
        }
      ],
      "options": {
        "updateBuildableProjectDepsInPackageJson": true,
        "outputPath": "dist/apps/auth",
        "main": "apps/auth/src/app.js",
        "tsConfig": "apps/auth/tsconfig.app.json",
        "buildableProjectDepsInPackageJsonType": "dependencies"
      }
    },
    "build-fastify": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "tsc -p apps/auth/tsconfig.app.json",
          "tsc -p apps/auth/tsconfig.app.json"
        ],
        "parallel": false
      }
    },
    "serve-fastify": {
      "executor": "@nrwl/workspace:run-commands",
      "options": {
        "commands": [
          "tsc -p apps/auth/tsconfig.app.json -w",
          "tsc-alias -p apps/auth/tsconfig.app.json -w",
          "fastify start dist/auth/apps/auth/src/app.js -P -l debug -w"
        ],
        "parallel": true
      }
    },
    "build2": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/auth",
        "main": "apps/auth/src/app.js",
        "tsConfig": "apps/auth/tsconfig.app.json",
        "webpackConfig": "apps/auth/webpack.config.cjs",
        "assets": [],
        "sourceMap": false,
        "optimization": false,
        "inspect": false,
        "minimize": false,
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true
        }
      }
    },
    "build": {
      "executor": "@nrwl/node:webpack",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/apps/auth",
        "main": "apps/auth/src/app.js",
        "tsConfig": "apps/auth/tsconfig.app.json",
        "assets": ["apps/auth/src/plugins", "apps/auth/src/rest", "apps/auth/src/repository"],
        "generatePackageJson": true
      },
      "configurations": {
        "production": {
          "optimization": true,
          "extractLicenses": true,
          "inspect": false
        }
      }
    },
    "serve": {
      "executor": "@nrwl/node:node",
      "options": {
        "buildTarget": "auth:build"
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/auth/**/*.js"]
      }
    },
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/apps/auth"],
      "options": {
        "jestConfig": "apps/auth/jest.config.js",
        "passWithNoTests": true
      }
    }
  },
  "tags": []
}
