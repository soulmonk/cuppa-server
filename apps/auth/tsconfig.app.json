{
  "extends": "./tsconfig.json",
  "compilerOptions": {
    "outDir": "../../dist/auth",
    "strict": false,
    "strictNullChecks": false,
    "exactOptionalPropertyTypes": false
  },
  "exclude": [
    "test",
    "*.config.js",
    "jest.config.ts",
    "**/*.spec.ts",
    "**/*.test.ts",
    "**/*.spec.js",
    "**/*.test.js"
  ],
  "include": ["**/*.ts", "**/*.js"]
}
