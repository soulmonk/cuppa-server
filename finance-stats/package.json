{
  "name": "finance-stats",
  "version": "1.0.0",
  "description": "",
  "main": "app.js",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "pretest": "standard",
    "fix": "standard --fix",
    "cover": "tap --cov --coverage-report=lcov",
    "db:migration": "node migration.js",
    "test": "node test/load-db.js && tap",
    "start": "fastify start -l info app.js",
    "dev": "fastify start -l info -P app.js",
    "schema": "apollo service:download --endpoint=\"http://localhost:3031/graphql\" --header=\"Authorization: Bearer ${TOKEN}\""
  },
  "keywords": [
    "api",
    "graphql",
    "finance"
  ],
  "author": "Vitalii Melnykov <fsoulmonk@gmail.com>",
  "license": "MIT",
  "dependencies": {
    "@graphql-tools/schema": "^8.3.1",
    "@graphql-tools/utils": "^8.6.1",
    "fastify": "^3.27.1",
    "fastify-autoload": "^3.10.0",
    "fastify-cors": "^6.0.2",
    "fastify-env": "^2.1.1",
    "fastify-jwt": "^4.1.3",
    "fastify-multipart": "^5.3.0",
    "fastify-plugin": "^3.0.1",
    "fastify-postgres": "^3.6.0",
    "fastify-redis": "^4.3.3",
    "fluent-json-schema": "^3.0.1",
    "googleapis": "^95.0.0",
    "graphql": "^16.3.0",
    "graphql-scalars": "^1.14.1",
    "hyperid": "^3.0.0",
    "lodash": "^4.17.21",
    "mercurius": "^9.2.0",
    "pg": "^8.7.3"
  },
  "devDependencies": {
    "fastify-cli": "^2.15.0",
    "apollo": "^2.33.9",
    "babel-eslint": "^10.1.0",
    "sinon": "^13.0.1",
    "standard": "*",
    "tap": "^15.1.6"
  },
  "standard": {
    "parser": "babel-eslint",
    "ignore": [
      "dist",
      "temp"
    ]
  },
  "tap": {
    "test-regex": ".*\\.test\\.js"
  }
}
